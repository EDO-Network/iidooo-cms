<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.iidooo.framework.dao.extend.FieldConfigDao" >
  <resultMap id="BaseResultMap" type="com.iidooo.framework.dto.extend.FieldConfigDto" >
    <id column="FieldID" property="fieldID" jdbcType="INTEGER" />
    <result column="ModelID" property="modelID" jdbcType="INTEGER" />
