<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="system" extends="yazaki">
        <action name="invoiceCodeRuleConfig" class="cn.co.yazaki.rivon.system.action.InvoiceCodeRuleConfigAction">
            <result name="success">/WEB-INF/jsp/system/InvoiceCodeRuleConfig.jsp</result>
        </action>
        <action name="invoiceUnitRuleConfig" class="cn.co.yazaki.rivon.system.action.InvoiceUnitRuleConfigAction" method="init">
            <result name="success">/WEB-INF/jsp/system/InvoiceUnitRuleConfig.jsp</result>
            <result name="input">/WEB-INF/jsp/system/InvoiceUnitRuleConfig.jsp</result>
            <result name="faild">/WEB-INF/jsp/system/InvoiceUnitRuleConfig.jsp</result>
        </action>
    </package>
</struts>